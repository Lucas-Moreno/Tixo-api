{
  "env": {
    "node": true
  },
  "extends": ["plugin:security/recommended", "plugin:jest/recommended",  "plugin:prettier/recommended"],
  "plugins": ["jest", "security", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": ["warn", "never"],
    "quotes": ["warn", "double"]
  }
}
